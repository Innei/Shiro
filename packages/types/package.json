{
  "name": "shiro-types",
  "version": "1.0.3",
  "exports": {
    ".": {
      "import": "./dist/index.mjs"
    },
    "./styles": "./dist/styles.css",
    "./sandbox": {
      "import": "./dist/sandbox.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "scripts": {
    "build": "tsup && cd ../.. && tailwindcss -i src/styles/index.css -o packages/types/dist/styles.css"
  },
  "devDependencies": {
    "tsup": "8.3.0"
  },
  "bump": {
    "before": [
      "pnpm run build"
    ],
    "changelog": false,
    "tag": false,
    "publish": true,
    "push": false,
    "allowDirty": true,
    "allowedBranches": [
      "main",
      "pro/dev"
    ]
  }
}
